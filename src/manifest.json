{
  "name": "插件中心",
  "version": "1.0.0",
  "manifest_version": 2,
  "homepage_url": "https://app.tigerkin.net/",
  "description": "插件中心",
  "permissions": [
    "history",
    "topSites",
    "cookies",
    "storage",
    "tabs",
    "notifications",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "management",
    "chrome://favicon/",
    "activeTab",
    "<all_urls>",
    "*://*/*"
  ],
  "icons": {
    "16": "icons/logo.png",
    "48": "icons/logo.png",
    "128": "icons/logo.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "web_accessible_resources": [
    "extend/*",
    "extend/xmlhttp.js"
  ],
  "content_security_policy": "script-src 'self' https://t.captcha.qq.com 'unsafe-eval'; object-src 'self'"
}
